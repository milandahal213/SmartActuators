void printout(char message[100]) {
  char pm[15];
  for (int j = 0; strlen(message) > mCount; j++) {
    strcpy(pm, ".");
    for (int i = 0; i < 15; i++) {
      if (i > 10 && message[mCount] == ' ') {
        pm[i] = ' ';
      }
      else {
        pm[i] = message[mCount];
        mCount = mCount + 1;
      }
    }
    tft.drawString(pm, 220, 100 + j * 10);
  }
  mCount = 0;
}



